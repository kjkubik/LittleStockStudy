# scheduler.py
import schedule
import time
from GetAllStocksDatas import process_this  # Importing the process_this function from GetAllStocksData
from config import stock_key

# Schedule the task to run daily at a specific time (e.g., 9:00 AM)
schedule.every().day.at("08:54").do(process_this)

# Keep the program running to allow scheduled tasks to execute
while True:
    schedule.run_pending()
    time.sleep(1)
    


